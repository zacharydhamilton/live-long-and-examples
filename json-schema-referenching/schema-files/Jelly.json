{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Jelly",
    "description": "com.github.zacharydhamilton.events",
    "type": "object",
    "version": "1",
    "properties": {
        "flavor": {
            "type": "string",
            "description": "The flavor of this Jelly (i.e. grape, strawberry, raspberry)."
        },
        "organic": {
            "type": "boolean",
            "description": "Is this organic or not."
        }
    },
    "required": ["flavor", "organic"]
}
